@model IEnumerable<CRM.Models.Domain.Employee>
@{
    ViewData["Title"] = "GetAllEmployees";
    Layout = "~/Views/Shared/_Admin.cshtml";
}





<div class="page-wrapper">
    <!-- Page header -->
    <div class="page-header d-print-none">
        <div class="container-xl">
            <div class="row g-2 align-items-center">
                <div class="col">
                    <h2 class="page-title">
                        EMPLOYEES
                    </h2>
                </div>
                <!-- Page title actions -->
                <div class="col-auto ms-auto d-print-none">
                    <div class="d-flex">
                        <input type="search" class="form-control d-inline-block w-9 me-3" placeholder="Search Employee..." />
                        <a href="#" class="btn btn-primary">
                            <!-- Download SVG icon from http://tabler-icons.io/i/plus -->
                            <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round"><path stroke="none" d="M0 0h24v24H0z" fill="none" /><path d="M12 5l0 14" /><path d="M5 12l14 0" /></svg>
                            New Employee
                        </a>
                    </div>
                </div>
            </div>
      </div>

    <!-- Page body -->
    <div class="page-body">
        <div class="container-xl">
            <div class="row row-cards">
                @foreach (var employee in Model)
                {
                    <div class="col-md-6 col-lg-3 mb-4">
                        <div class="card">
                            <div class="card-body p-4 text-center">
                                <span class="avatar avatar-xl mb-3 rounded" style="background-image: url('@Url.Content("~/Images/" + employee.PhotoPath)')"></span>

                                <h3 class="m-0 mb-1">
                                    <a href="#">@employee.Name</a>
                                </h3>
                                
                                <div class="mt-3">
                                    <div class="text-secondary">Designation:-</div><span class="badge bg-purple-lt">@employee.EmployeeDesignation</span>
                                </div>
                            </div>
                            <div class="d-flex justify-content-around p-3">
                                <a href="mailto:@employee.Email" class="btn btn-outline-secondary">
                                    <svg xmlns="http://www.w3.org/2000/svg" class="icon me-2 text-muted" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round"><path stroke="none" d="M0 0h24v24H0z" fill="none" /><path d="M3 7a2 2 0 0 1 2 -2h14a2 2 0 0 1 2 2v10a2 2 0 0 1 -2 2h-14a2 2 0 0 1 -2 -2v-10z" /><path d="M3 7l9 6l9 -6" /></svg>
                                    Email
                                </a>
                                <a href="tel:@employee.PhoneNumber" class="btn btn-outline-secondary">
                                    <svg xmlns="http://www.w3.org/2000/svg" class="icon me-2 text-muted" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round"><path stroke="none" d="M0 0h24v24H0z" fill="none" /><path d="M5 4h4l2 5l-2.5 1.5a11 11 0 0 0 5 5l1.5 -2.5l5 2v4a2 2 0 0 1 -2 2a16 16 0 0 1 -15 -15a2 2 0 0 1 2 -2" /></svg>
                                    Call
                                </a>
                            </div>
                            <div class="card-footer text-center">
                                <a href="@Url.Action("GetEmployeeDetails", "Admin", new { id = employee.EmployeeId })" class="btn btn-primary">
                                    More Details
                                </a>
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
    </div>

